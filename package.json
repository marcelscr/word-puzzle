{
  "name": "word-puzzle",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "lint": "eslint 'app/**/*.ts?(x)'",
    "lintFix": "eslint 'app/**/*.ts?(x)' --fix",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@remix-run/react": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "concurrently": "^7.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.9.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@types/lodash": "^4.14.181",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/seedrandom": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "yarn-upgrade-all": "^0.7.1"
  },
  "engines": {
    "node": ">=14"
  }
}
